<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.demo.subscriptionbackgroundflow">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application>
        <activity
            android:name=".activity.SubscriptionActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppThemeSubscription" />
        <activity
            android:name=".activity.SubscriptionBackgroundActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppThemeSubscription" />
        <activity
            android:name=".activity.PrivacyPolicyActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppThemeSubscription"/>
        <activity
            android:name=".activity.TermsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppThemeSubscription"/>
    </application>

</manifest>